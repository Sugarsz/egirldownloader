{
    "name": "E-girl Master",
    "version": "1.0",
    "description": "Baixe fotos de gostosas do Instagram",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "tab-icon.png",
        "default_popup": "popup.html" 
    },
    "background": {
      "scripts": ["scripts.js"],
      "persistent": false
    },
    "icons":{
      "256": "iconpic.png"
    },
    "permissions": [
        "activeTab",
        "declarativeContent"
    ]
  }